// configuration of a BackTest
{
    "logger": {
        "level": "debug", //40
        // "console": "False",
    },

    "trader": { // a trader operating an account by depending on advisor and marketEvents
        "objectives": ["SZ000001"], // ["SH510050"],

        "backTest":{
            "startBalance": 100000,
            "episodes": 1,
            "warmupDays": 60,
            // "startDate": "2017-01-01",
