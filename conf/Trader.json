            // "endDate"  : "2015-12-31",
            "ideal": "SinaFP", // "SinaFP", "SinaT+1", // "T+1",
        }
    },

    "account": { // account settings
        "id": "blah",
        "broker": "AShare",

        "slippage" : 0,       // 假设的滑点
